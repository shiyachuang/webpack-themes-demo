{
    "presets": [
        [
            "env",
            {
                "loose": true,  
                "modules": false 
            }
        ],
        "es2015",
        "react",
        "stage-0",
    ],
    "plugins": [
        "transform-runtime",
        "react-hot-loader/babel",
        "transform-decorators-legacy",
        [
            "import",
            {
              "libraryName": "antd",
              "libraryDirectory": "es",
              "style": "css"
            }
          ]
    ]
}