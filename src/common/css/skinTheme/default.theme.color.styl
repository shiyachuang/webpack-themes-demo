          .bor-drag
            background $bor-drag
          &:hover
            background $value-drag-hover-bg
            border-color $value-drag-hover-bor
          &.blues
            background $value-drag-bg
            border 1px solid $value-drag-bor
            color $value-drag-txt-col
            .bor-drag
              background $bor-drag
            .vbor-drag
              background $bor-drag
            &:hover
              background $value-drag-hover-bg
              border-color $value-drag-hover-bor
          &.greens
            background $values-box-bg
            border 1px solid $values-box-bor
            color $values-box-txt-col
            .bor-drag
              background $val-bor-drag
            .vbor-drag
              background $val-bor-drag
            &:hover
              background $values-box-hover-bg
              border-color $values-box-hover-bor
              color $values-box-txt-hover-col
      .values-box
        .values-title
          .add
            .iconFont:hover
              color $main-color
        ul.values-list
          li
            background $values-box-bg
            border 1px solid $values-box-bor
            color $values-box-txt-col
            .vbor-drag
              background $val-bor-drag
            &:hover
              background $values-box-hover-bg
              border-color $values-box-hover-bor
              color $values-box-txt-hover-col
            &.blues
              background $value-drag-bg
              border 1px solid $value-drag-bor
              color $value-drag-txt-col
              .bor-drag
                background $bor-drag
              .vbor-drag
                background $bor-drag
              &:hover
                background $value-drag-hover-bg
                border-color $value-drag-hover-bor
            &.greens
              background $values-box-bg
              border 1px solid $values-box-bor
              color $values-box-txt-col
              .bor-drag
                background $val-bor-drag
              .vbor-drag
                background $val-bor-drag
              &:hover
                background $values-box-hover-bg
                border-color $values-box-hover-bor
                color $values-box-txt-hover-col
    //add card right
    .right-tab
