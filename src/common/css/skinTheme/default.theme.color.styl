// @require "./../comm"
body
  background $body-bg
  overflow-x hidden;
//header+nav
header
  background linear-gradient(to right, $nav-dark-bg, $nav-linght-bg)
  .header-top
    background linear-gradient(to right, $nav-dark-bg, $nav-linght-bg)
    //用户头像
    .user-box
      .user-pop
        .iconFont.icon-checkbox
          color $main-color
        .group-lists li:hover
          a
            color $main-color
        .group-title i.iconFont:hover
           color $main-color
    .avatar-com .avatar-content .user-pop .user-content ul.group-lists li:hover{
      a {
            color $main-color
      }
    }
    .avatar-com .avatar-content .user-pop .user-content .group-title {
      i.iconFont:hover {
        color $main-color
      }
    }
    //九宫格模块
    .action-box, .sudoku-com
      .action-module
        .img-box
          i.iconFont
            color $action-box-iconCol
        .name-txt
          color $body-txt
        &:hover
          background $action-module-hoverBg
          .name-txt
            color $main-color
      .botm-liner
        background $set-box-liner-bg
  //nav(menu)
  .navigation
    background linear-gradient(to right, $nav-dark-bg, $nav-linght-bg)
    .header-top-bottom
      background $header-top-bottom
    .li-hover.active,.li-hover:hover
      background-color $nav-hover-color
      a
        color $nav-a-col
    .nav-mask
      background $nav-mask
    //more-nav
    li.li-more
      ul.more-menubox
        background $nav-more-bg
        .top-search
          .lin-span
            background $fff
        .morbox
          li.active
            background-color $nav-more-hover-color
            &:hover
              background-color $nav-more-hover-color
        &:hover
          li:hover
            background-color $nav-more-hover-color
            span
              color $nav-a-col
            .m-close
              .iconFont
                color  $nav-more-close-icon
              &:hover
                .iconFont
                  color  $nav-a-col
    li.li-more.m-active
      a.a-more
        background $nav-more-bg
    li.li-more.active
      background-color $nav-hover-color
    li.li-more:hover
      background-color $nav-hover-color
    //拖拽的线
    .menu-drag
      background rgba(255, 255, 255, .6)
    .more-menu-drag
      background rgba(0, 0, 0, .2)
//内容部分
.main-box
  .cards-top
